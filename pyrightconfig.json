{
  "include": ["src", "config"],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".venv",
    "working_files"
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "pythonPlatform": "All",
  "executionEnvironments": [
    {
      "root": "src"
    }
  ]
}